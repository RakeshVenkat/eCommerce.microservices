{
  "name": "express-ts-examples",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:dev": "nodemon --exec babel-node -- index.js",
    "start": "npm run build && node index.js",
    "build": "npm run clean && babel src -d build -s --source-maps --copy-files",
    "clean": "rm -rf build && mkdir build"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "hjs": "0.0.6",
    "hogan-middleware": "^0.2.2",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "joi": "^14.3.1",
    "morgan": "~1.9.1",
    "pug": "^3.0.1",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-express": "^4.1.3",
    "uuid": "^7.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.6"
  }
}
